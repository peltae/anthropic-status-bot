# Discord Bot Token (Required)
# Get this from Discord Developer Portal: https://discord.com/developers/applications
DISCORD_TOKEN=your_bot_token_here

# Discord Channel ID (Required)
# Right-click on the channel in Discord and select "Copy ID"
DISCORD_CHANNEL_ID=your_channel_id_here

# Check Interval in Minutes (Optional)
# How often to check for status updates (default: 5)
CHECK_INTERVAL=5

# Logging Level (Optional)
# Available levels: debug, info, warn, error (default: info)
# - debug: Detailed debugging information
# - info: General operational information
# - warn: Warning messages for potentially problematic situations
# - error: Error messages for serious problems
LOG_LEVEL=info